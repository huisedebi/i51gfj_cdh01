<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/margin_size_12dp"
    >
    <View
        android:layout_width="@dimen/margin_size_1dp"
        android:layout_height="@dimen/margin_size_12dp"
        android:background="@color/black">

    </View>
    <TextView
        android:id="@+id/center_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="菜单"
        android:layout_centerInParent="true"
        />
    <ImageView
        android:layout_width="@dimen/margin_size_8dp"
        android:layout_height="@dimen/margin_size_8dp"
        android:layout_alignParentRight="true"
        android:src="@drawable/ic_select"
        />






</RelativeLayout>



